generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id      Int      @id @default(autoincrement())
  name    String   @unique
  salt    String   @unique
  session String   @unique
  hash    String
  tests   Test[]
}

model Test {
  id      Int      @id @default(autoincrement())
  WPS     Int
  date    DateTime
  user    User?    @relation(fields: [userId], references: [id])
  userId  Int?
}