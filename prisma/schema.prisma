generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id      Int      @id @default(autoincrement())
  name    String   @unique
  salt    String   @unique
  session String   @unique
  hash    String
}

model Test {
  id      Int      @id @default(autoincrement())
  length  Int
  time    Float
  date    DateTime
}